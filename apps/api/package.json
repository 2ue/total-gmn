{
  "name": "@total-gmn/api",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "predev": "pnpm --filter @total-gmn/shared build",
    "dev": "tsx watch src/server.ts",
    "prebuild": "pnpm --filter @total-gmn/shared build && pnpm prisma generate",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/server.js",
    "lint": "echo 'No lint configured'",
    "format": "echo 'No format configured'",
    "test": "vitest run",
    "prisma": "prisma",
    "migrate:dev": "prisma migrate dev",
    "report": "tsx src/scripts/stage1-report.ts"
  },
  "dependencies": {
    "@fastify/cors": "^11.0.1",
    "@fastify/multipart": "^9.0.3",
    "@prisma/client": "^6.13.0",
    "@total-gmn/shared": "workspace:*",
    "csv-parse": "^5.5.6",
    "fastify": "^5.0.0",
    "iconv-lite": "^0.6.3",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "prisma": "^6.13.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "vitest": "^2.1.8"
  }
}
